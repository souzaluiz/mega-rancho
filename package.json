{
  "name": "mega-rancho",
  "version": "1.0.0",
  "main": "src/index.js",
  "scripts": {
    "build:prod": "webpack --config ./webpack/webpack.prod.js",
    "build:dev": "webpack --config ./webpack/webpack.dev.js -w",
    "start": "node src/index.js",
    "server": "nodemon src/index.js",
    "web": "browser-sync start --proxy 'localhost:3333' --files 'public/css/*.min.css, src/view/*.njk'",
    "dev": "npm-run-all -p build:dev server web"
  },
  "license": "MIT",
  "dependencies": {
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.9.19",
    "multer": "^1.4.2",
    "node-telegram-bot-api": "^0.50.0",
    "nunjucks": "^3.2.1",
    "sharp": "^0.25.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.3",
    "@babel/core": "^7.10.3",
    "@babel/preset-env": "^7.10.3",
    "@babel/preset-react": "^7.10.1",
    "@types/react": "^16.9.41",
    "@types/react-dom": "^16.9.8",
    "babel-loader": "^8.1.0",
    "babel-minify-webpack-plugin": "^0.3.1",
    "browser-sync": "^2.26.7",
    "clean-webpack-plugin": "^3.0.0",
    "nodemon": "^2.0.4",
    "npm-run-all": "^4.1.5",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
